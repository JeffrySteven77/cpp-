#include <iostream>

using namespace std;

int suma (int A, int B)
{
	return  A + B;
}

int main ( int argc, char const *argv[])

{
    system ("cls");
	
	cout << suma (77, 23);
	cout << endl;
	cout << suma (7, 3);
	cout << endl;
	cout << suma (55, 35);
	cout << endl;
	cout << suma (12, 12);
	cout << endl;
	
	return 0;
}
