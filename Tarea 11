#include <iostream>
#include <stdlib.h>
#include <time.h>

using namespace std;

int main ( int argc, char const * argv[])
{
	int A [5][5];
	
	srand (time (NULL));
	
	int Numero_Ramdom = 0;
    int Total_Suma = 0;
	
	for (int I = 0; I < 5; I++)
	{
		for (int J = 0; J < 5; J++)
		{
			Numero_Ramdom = rand() % 5 + 1;
			A[I][J] = Numero_Ramdom;
			Total_Suma = Total_Suma + (A[I][J]);
		}
	}
	
	for (int I = 0; I < 5; I++)
	{		
		for (int J = 0; J < 5; J++)
		{
	    	cout <<" " << A[I][J];
			
		}
		cout<<endl;
	}
   cout<<endl;
   cout << "TOTAL: ";
   cout << Total_Suma;
	return 0;
}
